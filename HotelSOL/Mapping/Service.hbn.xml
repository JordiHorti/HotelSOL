<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="HotelSOL" namespace="HotelSOL.Models">

	<class name="Service" table="services">
		<id name="services_id" column="services_id" type="Int32">
			<generator class="identity" />
		</id>
		<property name="service" column="service" type="String" />
		<property name="servicePrice" column="servicePrice" type="Decimal" />
	</class>

</hibernate-mapping>

